{
  "name": "json-server-gui",
  "description": "",
  "version": "0.1.0",
  "main": "server.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "node ./postinstall.js",
    "start": "node server.js"
  },
  "bin": {
    "json-server-gui": "./cli.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "basscss": "^8.0.2",
    "copyfiles": "^2.2.0",
    "core-js": "^2.6.5",
    "element-ui": "^2.4.5",
    "es6-promise": "^4.2.8",
    "faker": "^4.1.0",
    "find-and": "^1.3.0",
    "highlight.js": "^9.18.1",
    "http": "0.0.1-security",
    "lodash.clonedeep": "^4.5.0",
    "quicktype-core": "^6.0.52",
    "serve-handler": "^6.1.2",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-highlight-component": "^1.0.0",
    "vue-property-decorator": "^9.0.2",
    "vue-router": "^3.1.2",
    "vuedraggable": "^2.24.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "json-server": "^0.15.1",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "files": [
    "dist/**/*"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    },
    "plugins": [
      "vue"
    ],
    "rules": {
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "keyword-spacing": "warn",
      "no-multiple-empty-lines": "warn",
      "padded-blocks": "off",
      "semi": [
        "warn",
        "always"
      ],
      "space-before-blocks": "warn",
      "space-before-function-paren": "warn",
      "space-infix-ops": "warn",
      "space-unary-ops": "warn",
      "spaced-comment": "warn",
      "vue/max-attributes-per-line": "off",
      "vue/singleline-html-element-content-newline": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simoneldevig/json-server-gui.git"
  },
  "author": "Simon Eldevig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simoneldevig/json-server-gui/issues"
  },
  "homepage": "https://github.com/simoneldevig/json-server-gui#readme"
}
